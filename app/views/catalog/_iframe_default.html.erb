<%
iframe_url = nil
if document.has_key?('full_image_url_ssm')
	iframe_url_array = document.fetch('full_image_url_ssm')
	iframe_url = iframe_url_array[0]
end

%>
<div class="iframe-wrapper">
<iframe
    src="<%=iframe_url%>"
    allowfullscreen="true"
    width="100%"
    height="600px"
  ></iframe>
</div>
