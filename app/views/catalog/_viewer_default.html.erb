<% if document.uploaded_resource? %>
  <%= render_document_partials document, blacklight_config.view_config(:show).partials %>
<% else %>
  <%= 
    render_document_partials @document,
    blacklight_config.view_config(:show).partials.map!{ |partial| partial == :openseadragon ? "iframe_default" : partial },
    locals: { document: @document }
  %>
<% end %>